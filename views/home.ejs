    <main>
        <div id="description">
            <h3>Recent blog post</h3>
            <h5>Blog posts are arranged from the newest to the oldest!!</h5>
        </div>
        <div id="blogPosts">
            <% blogPosts.forEach((post)=>{%>
                <div class="blogPost">
                    <img src="<%= post.contentImageUrl %>" alt="content">
                    <h2><%= post.tittle %></h2>
                    <a href="/article/<%= post._id %>/<%=post.tittle %>">Read article here</a>
                </div>
                <%})%>
        </div>
